{
  "name": "aileron-airways-app",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://aileron-airways-app.herokuapp.com/",
  "dependencies": {
    "bulma": "0.6.2",
    "bulma-extensions": "1.0.13",
    "classnames": "2.2.5",
    "font-awesome": "4.7.0",
    "fuse.js": "3.2.0",
    "ideagen-timeline-sdk": "1.1.0",
    "lodash": "4.17.5",
    "moment": "2.20.1",
    "prop-types": "15.6.0",
    "react": "^16.2.0",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "jest-cli": "22.4.2",
    "jsdoc": "3.5.5",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "watch-css": "npm run build-css && node-sass-chokidar --watch --recursive src/styles/index.scss src/build-index.css",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/styles/index.scss src/build-index.css",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
