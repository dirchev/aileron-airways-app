{
  "name": "aileron-airways-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dirchev.github.io/aileron-airways-app/",
  "dependencies": {
    "bulma": "0.6.2",
    "classnames": "2.2.5",
    "font-awesome": "4.7.0",
    "prop-types": "15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1"
  },
  "devDependencies": {
    "gh-pages": "1.1.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "watch-css": "npm run build-css && node-sass-chokidar --watch --recursive src/styles/index.sass src/styles/index.css",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/styles/index.sass src/styles/index.css",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
